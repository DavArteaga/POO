package Ejercicios_Clase;

public class Ejercicio_14 {

    public static void main(String args[]) {
        //Declarar Variables
        double X, Cuadrado, Cubo;
        
        //Operaciones
        X = 5;
        Cuadrado = Math.pow(X,2);
        Cubo = Math.pow(X,3);
        
        //Imprimir en pantalla
        System.out.println("El Cuadrado del número "+X+" es: "+ Cuadrado);
        System.out.println("El Cubo del número "+X+" es: "+ Cubo);
    }
}
