package Ejercicios_Clase;

public class Ejercicio_5 {

    public static void main(String args[]) {
        //Declarar variables
        double Suma, X, Y;
        
        //Operaciones con las variables
        Suma = 0;
        X = 20;
        Suma = Suma + X;
        Y = 40;
        X = X + Math.pow(Y, 2);
        Suma = Suma + X / Y;
        
        //Imprimir en pantalla
        System.out.println("El valor de la suma es: "+Suma);
    }
}
